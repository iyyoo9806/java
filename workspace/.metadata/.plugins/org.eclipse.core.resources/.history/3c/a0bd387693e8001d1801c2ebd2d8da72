public class Product {
	String name;
	int price;
	int stock;
	int discount;
	double realPrice;

	Product(String name, int price, int stock, int discount, double realPrice) {
		this.name = name;
		this.price = price;
		this.stock = stock;
		this.discount = discount;
		this.realPrice = realPrice;
	}
}
