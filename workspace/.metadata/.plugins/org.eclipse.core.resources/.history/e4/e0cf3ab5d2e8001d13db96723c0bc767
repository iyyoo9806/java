package zNote;

public class Awq {
	public static void main(String[] args) {
		String[] b = { "ayaye", "uuuma", "ye", "yemawoo", "ayaa" };
		String d = "";
		d = b[0].replace("aya", "?");
		System.out.println(d);
		d = b[0].replace("ye", "?");
		System.out.println(d);
		int count = 0;
		for (int i = 0; i < b.length; i++) {
			String c = "";
			c = b[i].replace("aya", "?");
			c = b[i].replace("ye", "?");
			c = b[i].replace("woo", "?");
			c = b[i].replace("ma", "?");
			System.out.println(c);
		}
	}
}
