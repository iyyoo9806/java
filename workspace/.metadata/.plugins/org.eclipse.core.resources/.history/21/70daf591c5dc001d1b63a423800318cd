package zNote;

import java.util.Arrays;

public class A {
	public static void main(String[] args) {
		int start = 2;
		int n = 5;
		int sum = 0;
		int[] a = new int [5];
		for(int i=0; i<5; i++) {
			if(i==0) {
				a[i] = start;
			}else {
				a[i] = a[i-1]+1;
			}
		}
		System.out.println(Arrays.toString(a));
	}
}