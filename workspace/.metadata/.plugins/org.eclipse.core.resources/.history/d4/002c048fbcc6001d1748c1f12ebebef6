
public class Array2 {
public static void main(String[] args) {
	
	int num [] = new int[5];//index를 0부터 , zero-based index
	num[0] = 2;
	num[1] = 3;
	num[2] = 1;
	num[3] = 5;
	num[4] = 4;
	int min = 0 ,max = 0;
	
	// 배열의 최대 최소 값을 출력해주세요.
	for(int i = 0; i<num.length; i++) {
		if(num[i] < min) {
			min = num[i];
		}
		if(num[i] > max) {
			max = num[i];
		}
		
	}System.out.println("Max Value is"+ max);
	System.out.println("Min Value is"+ min);
}
}

