package quiz0516;

public class SelectedRoom {
	String name;
	String studentName;
	int score;
	
	SelectedRoom(String name, String studentName, int score){
		this.name = name;
		this.studentName = studentName;
		this.score = score;
	}
	
	@Override
	public String toString() {
		return this.studentName + " (" + this.score + ")";
	}
}
