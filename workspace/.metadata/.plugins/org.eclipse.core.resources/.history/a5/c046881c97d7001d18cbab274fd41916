package package2;

import java.util.Arrays;

public class Aa {
public static void main(String[] args) {
	String string = "try hello world";
	String[] s1 = string.split(" ");
	System.out.println(s1);
	int [] sLen = new int [s1.length];
	for(int i=0; i<s1.length; i++) {
		sLen[i] = s1[i].length();
	}
	
}
}
