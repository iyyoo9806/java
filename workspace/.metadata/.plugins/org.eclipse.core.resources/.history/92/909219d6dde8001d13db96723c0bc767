package zNote;

import java.util.Arrays;

public class Asd {
	public static void main(String[] args) {
		String[] park = {"OSO","OOO","OXO","OOO"};
		String[] routes = {"E 2","S 3","W 1"};
		int y = park.length;
		int x = park[0].length();
		String[][] a = new String[y][x];
		
		for(int i=0; i<y; i++) {
			for(int j=0; j<x; j++) {
				a[i][j] = park[j];
			}
		}
		System.out.println(Arrays.toString(a));
	}
}
