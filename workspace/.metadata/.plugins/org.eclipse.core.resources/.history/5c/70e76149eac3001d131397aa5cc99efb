package Jw2;

public class Jwparc3 {
	public static void main(String[] args) {

		int n = 7;
		if (n == 7) {
			for (int i = 1; i < 10; i++) {
				System.out.println(n + " x " + i + " = " + n * i);

			}
		}
		System.out.println("-----------------------------------");
		if(n+1 == 8) {
			for (int i = 1; i < 10; i++) {
				System.out.println((n+1) + " x " + i + " = " + (n+1) * i);
		}
	}System.out.println("-----------------------------------");
	if(n+2 == 9) {
		for (int i = 1; i < 10; i++) {
			System.out.println((n+2) + " x " + i + " = " + (n+2) * i);
}
} System.out.println("-----------------------------------");
}
}