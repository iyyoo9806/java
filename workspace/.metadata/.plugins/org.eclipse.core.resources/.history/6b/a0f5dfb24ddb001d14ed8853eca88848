package zNote;

import java.util.Arrays;

public class A {
	public static void main(String[] args) {
		String rsp = "205";
		String[] a = rsp.split("");
		String answer = "";
		
		for(int i=0; i<a.length; i++) {
			if(a[i].equals("2")) {
				answer = answer +"0";
			}else if(a[i].equals("0")) {
				answer = answer + "5";
			}else {
				answer = answer + "2";
			}
		}
		System.out.println(answer);
	}
}