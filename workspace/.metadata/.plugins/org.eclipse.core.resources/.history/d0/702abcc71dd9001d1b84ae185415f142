package zNote;

public class Q {
	public static long solution(long n) {
		long answer = 0;
		
		for (int i=0; i<999999; i++) {
			if (i * i == n) {
				answer = (i + 1) * (i + 1);
				break;
			} else if(i==500) {
				answer = -1;
				break;
			}
//			else {
//				return i=i+1;
//			}
		}
	
		return answer;
	}

	public static void main(String[] args) {
		System.out.println(solution(144));
	}
}
