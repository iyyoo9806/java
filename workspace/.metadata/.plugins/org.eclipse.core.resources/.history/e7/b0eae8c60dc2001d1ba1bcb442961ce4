import java.util.Scanner;

public class Main3 {
public static void main(String[] args) {
	Scanner p = new Scanner(System.in);
	System.out.println(p);
	
	int a = p.nextInt();
	
	if (a>=90) {
		System.out.println("A");
	}else if (a>=80) {
		System.out.println("B");
	}else if (a>=70) {
		System.out.println("C");
	} else {
		System.out.println("D");
	}
	
}
}
