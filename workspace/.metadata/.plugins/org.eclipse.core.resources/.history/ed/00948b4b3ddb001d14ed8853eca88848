package zNote;

import java.util.Arrays;

public class A {
	public static void main(String[] args) {
		String answer = "";
		int n = 3;
		String my_string = "hello";
		String[] a = my_string.split("");
		for(int i=0; i<a.length; i++) {
			for(int j=0; j<n; j++) {
				answer = answer + a[i];
			}
		}
		System.out.println(answer);
	}
}