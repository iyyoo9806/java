
public class Quiz3 {

	public int[] solution(int[] num_list) {
		int[] answer = {};
		int count1 = 0;
		int count2 = 0;
		for (int i = 0; i < num_list.length; i++) {
			if (num_list[i] % 2 == 0) {
				answer[0] = count1++;
			} else {
				answer[1] = count2++;
			}
		}
		return answer;
	}

	public static void main(String[] args) {
		System.out.println();
	}
}
