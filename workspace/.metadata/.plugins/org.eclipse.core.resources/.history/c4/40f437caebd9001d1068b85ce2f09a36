package zNote;

import java.util.Arrays;

public class F {
	public static String solution(String my_string, int n) {
        String answer = "";
        String[] a = my_string.split("");
        for(int i=0; i<n; i++){
            answer = a[i];
        }
        return answer;
    }

	public static void main(String[] args) {
		String my_string = "hello";
		System.out.println(solution(my_string,3));
	}
}
