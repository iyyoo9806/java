package zNote;

public class Awq {
	public static void main(String[] args) {
		String[] a = { "aya", "ye", "woo", "ma" };
		String[] b = { "ayaye", "uuuma", "ye", "yemawoo", "ayaa" };
		int count = 0;
		for (int i = 0; i < b.length; i++) {
			for(int j=0; j<a.length; j++) {
				String c = "";
				c = b[i].replace(a[j],"?");
				System.out.println(c);
			}
		}
		System.out.println(count);
	}
}
