package zNote;

import java.util.Arrays;

public class A {
	public static void main(String[] args) {
		
		int[] numbers = {1,2,3};
		int tmp = 0;
		for(int i=0; i<numbers.length; i++) {
			tmp = numbers[0];
			numbers[0] = numbers[numbers.length-1];
			numbers[numbers.length-1] = tmp;
		}
		
		System.out.println(Arrays.toString(numbers));
	}
}