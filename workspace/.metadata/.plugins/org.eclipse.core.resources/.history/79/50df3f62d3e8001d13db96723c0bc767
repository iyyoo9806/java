package zNote;

public class Awq {
	public static void main(String[] args) {
		String[] b = { "aya", "yee", "maa", "wyeoo"};

		int count = 0;
		for (int i = 0; i < b.length; i++) {
			String c = "";
			c = b[i].replace("aya", "?");
			c = c.replace("ye", "?");
			c = c.replace("woo", "?");
			c = c.replace("ma", "?");
			System.out.println(c);
			if(c.equals("")) {
				count++;
			}
		}
		System.out.println(count);
	}
}
