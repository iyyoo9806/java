package zNote;

public class F {
	 public static boolean solution(int x) {
	        boolean answer = true;
	        int sum=0;
	        if(x>0) {
	        	sum = x/10 + x%10;
	        }
	        if(x%sum == 0) {
	        	return true;
	        }else {
	        	return false;
	        }
	    }
	
	 
	 public static void main(String[] args) {
		System.out.println(solution(2));
	}
}
