package zNote;

import java.util.Arrays;

public class F {
public static void main(String[] args) {
	String string = "try hello world";
	String [] arr = string.split(" ");
	for(int i=0; i<arr.length; i++) {
		System.out.println(arr[i]);
	}
	for(int i=0; i<arr.length; i++) {
		 arr[i].split("");
	}
	System.out.println(Arrays.toString(arr));
}
}
