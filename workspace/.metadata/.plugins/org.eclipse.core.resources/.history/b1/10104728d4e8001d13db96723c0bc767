package zNote;

public class Awq {
	public static void main(String[] args) {
		String[] b = { "aya", "yee", "maa", "wyeoo"};
		String[] d = new String[b.length];
		int count = 0;
		for (int i = 0; i < b.length; i++) {
			String c = "";
			c = b[i].replace("aya", "/");
			c = c.replace("ye", "/");
			c = c.replace("woo", "/");
			c = c.replace("ma", "/");
//			c = c.replace("/", "");
			System.out.println(c);
		}
		System.out.println(count);
	}
}
