package Jw2;

public class Jwparc3 {
public static void main(String[] args) {
	for(int i = 1; i<4; i++)
		
		if(i == 1) {
			System.out.println("7 x " + i + "=" + 7*i );
		}else if(i==2) {
			System.out.println("8 x " + i + "=" + 8*i );
		}else {
			System.out.println("9 x " + i + "=" + 9*i);
		}
	
		
		
		
		
		
}
}
