
public class Star {
public static void main(String[] args) {
	for(int i=0; i<3; i++) {
	for(int j= 0; j<3; j++) {
		System.out.print("*");
	}
		System.out.println(" ");
	}
	
	int sum = 0;
	for(int i=0; i<5; i++) {
		System.out.print(i);
	}
	for(int i=0; i<5; i++) {
		sum = sum + i;
	}
	System.out.println(sum);

}
}
